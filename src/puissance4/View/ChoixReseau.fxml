<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="puissance4.Controller.ChoixReseauController">
   <center>
      <HBox alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#selectionServeur" prefHeight="40.0" prefWidth="80.0" text="Serveur" />
            <Button mnemonicParsing="false" onAction="#selectionClient" prefHeight="40.0" prefWidth="80.0" text="Client" />
            <Button mnemonicParsing="false" onAction="#accueil" prefHeight="40.0" prefWidth="80.0" text="Accueil" />
            <Button mnemonicParsing="false" onAction="#quitter" prefHeight="40.0" prefWidth="80.0" text="Quitter" />
         </children>
      </HBox>
   </center>
   <top>
      <Label text="Puissance 4" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
         <font>
            <Font name="System Italic" size="48.0" />
         </font>
      </Label>
   </top>
</BorderPane>
